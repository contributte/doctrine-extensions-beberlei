{
  "name": "nettrine/extensions-beberlei",
  "description": "Doctrine2 DQL functions for Nette Framework",
  "keywords": ["nette", "doctrine", "orm", "database"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/doctrine-extensions-beberlei",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.2",
    "beberlei/doctrineextensions": "^1.2.0",
    "doctrine/orm": "^2.5.0",
    "nette/di": "^3.0.0"
  },
  "require-dev": {
    "contributte/phpstan": "^0.1",
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.3",
    "tracy/tracy": "^2.10.3"
  },
  "autoload": {
    "psr-4": {
      "Nettrine\\Extensions\\Beberlei\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.3.x-dev"
    }
  }
}
